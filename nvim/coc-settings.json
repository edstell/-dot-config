{
	"workspace.bottomUpFiletypes": ["gopls"],
	"languageserver": {
		"flow": {
			"command": "npx",
			"args": [
				"flow",
				"lsp"
			],
			"filetypes": [
				"javascript",
				"javascriptreact"
			],
			"initializationOptions": {},
			"requireRootPattern": true,
			"settings": {},
			"rootPatterns": [
				".flowconfig"
			]
		}
	},
	"diagnostic-languageserver.filetypes": {
		"sh": "shellcheck",
		"javascript": "eslint",
		"javascript.jsx": "eslint"
	},
	"javascript.validate.enable": false,
	"coc.preferences.formatOnSaveFiletypes": [
		"css",
		"markdown",
		"javascript",
		"graphql",
		"yaml",
		"js",
		"golang",
		"html"
	]
}
